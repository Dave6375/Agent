{
  "name": "ai-travel-agent",
  "version": "1.0.0",
  "description": "A powerful AI travel assistant with real-time web search, weather data, and location services",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "node --watch app.js",
    "test": "NODE_ENV=test node --test test/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "audit": "npm audit",
    "security-check": "npm audit --audit-level moderate"
  },
  "keywords": [
    "ai",
    "travel",
    "agent",
    "openai",
    "telegram",
    "bot",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.1",
    "openai": "^4.73.1",
    "telegraf": "^4.16.3",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "@eslint/js": "^9.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
